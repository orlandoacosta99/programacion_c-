<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6" />
    </startup>

  
  <system.diagnostics>
    <trace autoflush="true">
      <listeners>
        <add name="MyCustomListener" type="System.Diagnostics.TextWriterTraceListener" traceOutputOptions="Callstack,DateTime" initializeData="log.txt"></add>
      </listeners>
    </trace>  
  </system.diagnostics>


  <appSettings>
    
    <add key="Folder" value="c:\kk" />
    <add key="WriteToFile" value="true" />
    
  </appSettings>
  
  
</configuration>